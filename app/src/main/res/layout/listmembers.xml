<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/expandible"
    android:orientation="vertical"
    tools:context=".view.inicio_logueados_estudiantes.Inicio_LogueadosFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:gravity="right"
        android:layout_margin="5dp">
        <ImageView
            android:id="@+id/image_contacto_miembro"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/contacto_lider" />
        <TextView
            android:id="@+id/nombre_miembro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- nombre miembro 1"
            android:textColor="@color/negro"
            android:fontFamily="sans-serif"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"/>
    </LinearLayout>

</LinearLayout>